{
  "name": "awaitajax",
  "version": "0.1.1",
  "description": "Turns Node http.request into single-callback form, for use with await/defer",
  "main": "lib/awaitajax.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --bail --require blanket --require iced-coffee-script/register test/**/*.iced"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doublerebel/node-awaitajax"
  },
  "keywords": [
    "iced",
    "coffeescript",
    "await",
    "defer",
    "ajax",
    "http",
    "xhr",
    "queue"
  ],
  "author": "doublerebel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doublerebel/node-awaitajax/issues"
  },
  "homepage": "https://github.com/doublerebel/node-awaitajax",
  "dependencies": {
    "najax": "alanclarke/najax",
    "iced-runtime": "~1.0.1"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.10.0",
    "iced-coffee-script": "1.8.0-c",
    "mocha": "^2.1.0",
    "nock": "^0.59.1"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  }
}
